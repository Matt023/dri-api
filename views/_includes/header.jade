.navbar
    .navbar-inner
        .container
            a(data-toggle='collapse',data-target='.nav-collapse').btn.btn-navbar
                span.icon-bar
                span.icon-bar
                span.icon-bar
            a.brand(href='/') DRI
            .nav-collapse
                ul.nav
                    - var obj = { '/': 'Home', '/dev':'Development' }
                        - each val, key in obj
                          - if (id == key) 
                            li.active
                              a(href='#{key}') #{val}
                          - else
                            li
                              a(href='#{key}') #{val}
